{
  "name": "rplayer",
  "description": "A record player",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.24.7",
    "@vue/cli-plugin-babel": "^5.0.8",
    "d3-color": "^2.0.0",
    "lodash": "4.17.20",
    "vue": "^2.7.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "vue-template-compiler": "^2.7.16",
    "webpack": "^5.92.0"
  },
  "babel": {},
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/no-mutating-props": "off"
    },
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "ecmaVersion": 2020,
      "sourceType": "module"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "vue",
    "vuejs",
    "starter"
  ]
}
